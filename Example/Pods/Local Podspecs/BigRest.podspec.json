{
  "name": "BigRest",
  "version": "1.0.0",
  "summary": "A library for mapping RESTful JSON to NSManagedObjects. Go ahead, take a BigRest.",
  "description": "A library that simplifies REST mapping to NSManagedObject subclasses.",
  "homepage": "https://github.com/bigworkindustries/BigRest",
  "license": "MIT",
  "authors": {
    "Vincil Bishop": "vincil@bigworkindustries.com"
  },
  "source": {
    "git": "https://github.com/bigworkindustries/BigRest.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "BigRest": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
